# AWS 설정
AWS_REGION=ap-northeast-2
AWS_ACCOUNT_ID=your-account-id

# DynamoDB 테이블
DYNAMODB_TABLE_NAME=hanbit-todo-table

# JWT 설정
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=24h

# 환경 설정
NODE_ENV=development
LOG_LEVEL=info

# CORS 설정
ALLOWED_ORIGINS=http://localhost:5173,https://yourdomain.com

# API Gateway 설정
API_STAGE=dev